SELECT D.COMPTE AS NUMERO_DE_COMPTE,C.AGENCE AS CODE_AGENCE,
D.NBRCHQ AS NBRE_CARNET,D.COMPTE,D.REFER2 - D.REFER1 AS NBRE_FEUILES,
01 AS CODE_TRANSACTION ,D.ADRL1 AS NOMCLIENT ,D.ADRL3 AS ADDRESSE,'929' AS CODE_DEVISE,'00026' AS CODE_BANK,02 AS CODE_PAYS ,D.REFER1 AS NUMERO_DE_DEBUT,D.STATE AS STATE
FROM DEMCHQ D, DEMCHQDTL l, cpt c
where D.CLIENT = c.client and   d.nooper = l.checkbk_nooper
--and  D.COMPTE='00001460008' OR D.COMPTE='00001510001' OR D.COMPTE='00001550003' 
 and D.DATEDDEM IS NOT NULL and D.STATE='V' and l.state = 'V' and l.valide = 'V' AND D.DATREMCL IS NULL
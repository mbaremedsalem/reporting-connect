SELECT D.COMPTE AS NUMERO_DE_COMPTE,C.CODE_AGENCE AS CODE_AGENCE,
D.NBRCHQ AS NBRE_CARNET,(CASE WHEN D.TYPCHQ = 10 THEN '25' ELSE '50' END) AS NBRE_FEUILES,
01 AS CODE_TRANSACTION ,c.NOM AS NOMCLIENT ,D.ADRL3 AS ADDRESSE,
929 AS CODE_DEVISE,'00026' AS CODE_BANK,'02' AS CODE_PAYS,
CONCAT('000',D.REFER1 )AS NUMERO_DE_DEBUT, DT.STATUS status ,D.state statu1,D.DATDEM date_demande,D.DATEDDEM DATE_ENVOYE,D.DATREDEM DATE_RECEPTION, D.DATREMCL DATE_DISTRIBUTION
FROM cpt,DEMCHQ D,DEMCHQDTL DT,client C
WHERE   
cpt.CLIENT = c.CODE_RACINE and 
D.compte = cpt.compte
and  D.NOOPER = DT.CHECKBK_NOOPER
and DT.STATUS='ST'
and
D.COMPTE='00002610001';

--------------demandez chequier client
SELECT D.COMPTE AS NUMERO_DE_COMPTE,cpt.agence AS CODE_AGENCE,
D.NBRCHQ AS NBRE_CARNET,(CASE WHEN D.TYPCHQ = 10 THEN '25' ELSE '50' END) AS NBRE_FEUILES,
01 AS CODE_TRANSACTION ,c.NOM AS NOMCLIENT ,D.ADRL3 AS ADDRESSE,
929 AS CODE_DEVISE,'00026' AS CODE_BANK,'02' AS CODE_PAYS,
CONCAT('000',DT.REFER1 )AS NUMERO_DE_DEBUT, DT.STATUS status ,D.state statu1,D.DATDEM date_demande,D.DATEDDEM DATE_ENVOYE,D.DATREDEM DATE_RECEPTION, D.DATREMCL DATE_DISTRIBUTION
FROM cpt,DEMCHQ D,DEMCHQDTL DT,client C
where  cpt.CLIENT = c.CODE_RACINE and 
D.compte = cpt.compte
and  D.NOOPER = DT.CHECKBK_NOOPER
and DT.STATUS='ST'
and D.CLIENT <> '218009';

--------------demandez chequier pour cheque de banque
SELECT D.COMPTE AS NUMERO_DE_COMPTE,cpt.agence AS CODE_AGENCE,
D.NBRCHQ AS NBRE_CARNET,(CASE WHEN D.TYPCHQ = 10 THEN '25' ELSE '50' END) AS NBRE_FEUILES,
01 AS CODE_TRANSACTION ,c.NOM AS NOMCLIENT ,D.ADRL3 AS ADDRESSE,
929 AS CODE_DEVISE,'00026' AS CODE_BANK,'02' AS CODE_PAYS,
CONCAT('000',DT.REFER1 )AS NUMERO_DE_DEBUT, DT.STATUS status ,D.state statu1,D.DATDEM date_demande,D.DATEDDEM DATE_ENVOYE,D.DATREDEM DATE_RECEPTION, D.DATREMCL DATE_DISTRIBUTION
FROM cpt,DEMCHQ D,DEMCHQDTL DT,client C
where  cpt.CLIENT = c.CODE_RACINE and 
D.compte = cpt.compte
and  D.NOOPER = DT.CHECKBK_NOOPER
and DT.STATUS='ST'
and D.CLIENT = '218009';



